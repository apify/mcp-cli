{
  "mcpServers": {
    "example-http": {
      "url": "https://mcp.example.com",
      "headers": {
        "Authorization": "Bearer ${EXAMPLE_API_TOKEN}",
        "X-Custom-Header": "test-value"
      },
      "timeout": 60
    },
    "example-stdio": {
      "command": "node",
      "args": ["examples/test-server/index.js"],
      "env": {
        "DEBUG": "mcp:*",
        "NODE_ENV": "development"
      }
    }
  }
}
